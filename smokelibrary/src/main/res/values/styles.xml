<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android = "http://schemas.android.com/apk/res/android">
    <style name="dlg_refresh_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="my_self_dialog" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@null</item>
        <item name="android:backgroundDimEnabled">true</item><!--模糊-->
        <item name="android:windowBackground"> @android:color/transparent </item><!--背景透明-->
    </style>


    <!--动态标签Start-->
    <!--<item name="tag_textColor">#49C120</item>-->
    <style name="FlowlayoutTags">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="tag_textColor">#3577ce</item>
        <item name="tag_backgroundColor">#FFFFFF</item>
        <item name="tag_inputHintColor">#80000000</item>
        <item name="tag_inputTextColor">#DE000000</item>
        <item name="tag_checkedTextColor">#FFFFFF</item>
        <item name="tag_checkedBackgroundColor">#3577ce</item>
        <item name="tag_pressedBackgroundColor">#EDEDED</item>
        <item name="tag_textSize">@dimen/font_12sp</item>
        <item name="tag_horizontalSpacing">@dimen/horizontal_20</item>
        <item name="tag_verticalSpacing">@dimen/vertical_16</item>
        <item name="tag_horizontalPadding">@dimen/horizontal_10</item>
        <item name="tag_verticalPadding">@dimen/vertical_10</item>
        <item name="tag_radius">@dimen/vertical_4</item>
        <item name="tag_strokeColor">@drawable/gray_white_selector</item>
        <item name="tag_checkedStrokeColor">#3577ce</item>
        <item name="tag_strokeWidth">1px</item>
        <item name="tag_multiChooseable">true</item>
        <item name="tag_singleLine">true</item>
        <item name="tag_width">wrap_content</item>
        <item name="tag_height">wrap_content</item>
        <!--<item name="tag_width">@dimen/horizontal_100</item>-->
        <!--<item name="tag_height">@dimen/vertical_30</item>-->
    </style>

